# Navigation
alias ls='exa'
alias ll='exa -l'
alias lal='exa -alh'
alias cat='bat'

# Python
alias python='python3'
alias pip='pip3'
alias vba='source venv/bin/activate'

# Kubernetes
alias k='kubectl'

# Git
alias gc='git commit'
alias gca='git commit -a'
alias gcm='git commit -m'
alias gf='git fetch'
alias gm='git merge'
alias gs='git status'

